{
  "presets": ["es2015","stage-0"],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }],
    [
      "syntax-dynamic-import"
    ],
    ["transform-decorators-legacy"],
    ["transform-class-properties", { "spec": true }]
  ]
}